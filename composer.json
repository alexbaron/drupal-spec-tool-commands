{
    "name": "alexbaron77/drupal-spec-tool-commands",
    "description": "Provides composer commands for automating usage of acquia/drupal-spec-tool",
    "type": "composer-plugin",
    "require": {
        "google/apiclient": "^2.0",
        "composer-plugin-api": "^2.1"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Alexandre Baron",
            "email": "alexbaron77@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlexBaron77\\DrupalSpecToolCommands\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "class": "AlexBaron77\\DrupalSpecToolCommands\\ComposerPlugin"
    }
}
